<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.dao.ParameterDao">
	
	<select id="selectParameter" resultType="com.ssm.entity.Parameter">
   		select * from `px_parameter`
	</select>
	<select id="selectParameterOne" parameterType="int" resultType="com.ssm.entity.Parameter">
	  	select * from `px_parameter`  where SerialID = #{SerialID}
	</select>
	<update id="updateParameter" parameterType="com.ssm.entity.Parameter">
	   	update `px_parameter` set Name=#{Name},Number=#{Number} where SerialID=#{SerialID}
	</update>
</mapper>